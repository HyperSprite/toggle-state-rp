{
  "author": "C. Dunavan",
  "name": "@hypersprite/open-state-rp",
  "homepage": "https://github.com/HyperSprite/open-state-rp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HyperSprite/open-state-rp.git"
  },
  "bugs": {
    "url": "https://github.com/HyperSprite/open-state-rp/issues"
  },
  "keywords": [
    "testing",
    "react",
    "basic"
  ],
  "license": "MIT",
  "description": "'open' state render props controler",
  "main": "dst/index.js",
  "version": "1.0.3",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "lint": "eslint src/* .jsx",
    "build": "rm -rf dst && babel src --out-dir dst --ignore spec.js",
    "build:watch": "npm run build -- -w",
    "preversion": "npm run test && npm run lint",
    "version": "npm run build",
    "postversion": "npm publish && rm -rf dst && git push"
  },
  "files": [
    "dst"
  ],
  "precommit": [
    "lint",
    "test"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "peerDependencies": true
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            "spec.js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "raf/polyfill",
      "./.enzyme"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.spec.*"
    ],
    "collectCoverage": true
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.1.0",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
